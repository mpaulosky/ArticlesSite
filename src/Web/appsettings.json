{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		},
		"Console": {
			"FormatterName": "json"
		},
		"IncludeScopes": true
	},
	"OpenTelemetry": {
		"UseAspireDefaults": true,
		"Exporters": {
			"Otlp": {
				"Endpoint": "http://localhost:4317"
			}
		}
	},
	"AllowedHosts": "*"
}