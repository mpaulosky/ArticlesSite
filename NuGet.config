<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<clear />
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
		<add key="github-mpaulosky" value="https://nuget.pkg.github.com/mpaulosky/index.json" protocolVersion="3" />
	</packageSources>
	<packageSourceMapping>
		<!-- Map all packages to nuget.org by default -->
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
		<!-- Map specific packages to github-mpaulosky if needed -->
		<!-- <packageSource key="github-mpaulosky">
      <package pattern="YourPackagePrefix.*" />
    </packageSource> -->
	</packageSourceMapping>
</configuration>
